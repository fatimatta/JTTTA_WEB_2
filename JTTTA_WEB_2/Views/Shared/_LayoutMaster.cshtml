<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    @{
        ViewBag.pathParent = Url.Content("~").Substring(0, Url.Content("~").Length - 1);
    }
    <!-- Global stylesheets -->
    <link href='@ViewBag.pathParent/ContectLayout/css/roboto.css' rel='stylesheet' type='text/css'>
    <link href="@ViewBag.pathParent/ContectLayout/css/icons/icomoon/styles.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/ContectLayout/css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/ContectLayout/css/core.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/ContectLayout/css/components.css" rel="stylesheet" type="text/css">
    <link href="@ViewBag.pathParent/ContectLayout/css/colors.css" rel="stylesheet" type="text/css">

    <link href="@ViewBag.pathParent/kendo/css/kendo.common.min.css" rel="stylesheet" type="text/css" />
    <link href="@ViewBag.pathParent/kendo/css/kendo.metro.css" rel="stylesheet" type="text/css" />
    <!-- /global stylesheets -->

    <script type="text/javascript" src="@ViewBag.pathParent/Scripts/JQUERY/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/kendo/js/kendo.all.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/kendo/js/kendo.aspnetmvc.min.js"></script>

    <!-- Core JS files -->
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/loaders/pace.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/core/libraries/bootstrap.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/loaders/blockui.min.js"></script>
    <!-- /core JS files -->
    <!-- Theme JS files -->
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/visualization/d3/d3.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/visualization/d3/d3_tooltip.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/forms/styling/switchery.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/forms/styling/uniform.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/forms/selects/bootstrap_multiselect.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/ui/moment/moment.min.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/plugins/pickers/daterangepicker.js"></script>
    <script type="text/javascript" src="@ViewBag.pathParent/ContectLayout/js/core/app.js"></script>
    <!-- /theme JS files -->
</head>
<body>
    <!-- Main navbar -->
    <div class="navbar navbar-inverse">
        <div class="navbar-header">
            <a class="navbar-brand" href="index.html" style="padding: 13px 15px">
                @*<img src="@ViewBag.pathParent/ContectLayout/images/logo_light.png" alt="">*@
                Validation Data JT TTA
            </a>
            <ul class="nav navbar-nav" style="margin-left: 0px">
                <li><a class="sidebar-control sidebar-main-toggle hidden-xs"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>
        </div>
        <div class="navbar-collapse collapse" id="navbar-mobile">
            <ul class="nav navbar-nav navbar-right">
                <li class="dropdown dropdown-user">
                    <a class="dropdown-toggle" data-toggle="dropdown">
                        <img src="@ViewBag.pathParent/ContectLayout/images/demo/users/face1.jpg" alt="">
                        <span>@Session["Name"]</span>
                        <i class="caret"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-right">
                        <li><a href="@ViewBag.pathParent/Login/Profiles"><i class="icon-user-plus"></i> Change Profile</a></li>
                        <li><a href="@ViewBag.pathParent/Login/logout"><i class="icon-switch2"></i> Logout</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Page container -->
    <div class="page-container">
        <!-- Page content -->
        <div class="page-content">
            <!-- Main sidebar -->
            <div class="sidebar sidebar-main">
                <div class="sidebar-content">
                    <!-- User menu -->
                    <div class="sidebar-user">
                        <div class="category-content">
                            <div class="media">
                                <a href="#" class="media-left"><img src="@ViewBag.pathParent/ContectLayout/images/demo/users/face1.jpg" class="img-circle img-sm" alt=""></a>
                                <div class="media-body">
                                    <span class="media-heading text-semibold">@Session["Name"]</span>
                                    <div class="text-size-mini text-muted">
                                        <i class="icon-pin text-size-small"></i> &nbsp; @Session["description"] - @Session["distrik"]
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /user menu -->
                    <!-- Main navigation -->
                    <div class="sidebar-category sidebar-category-visible">
                        <div class="category-content no-padding">
                            <ul class="navigation navigation-main navigation-accordion">
                                <!-- Main -->
                                <li class="navigation-header"><span>Main Navigation</span> <i class="icon-menu" title="Main pages"></i></li>
                                @Html.Raw(ViewBag.leftMenu)
                            </ul>
                        </div>
                    </div>
                    <!-- /main navigation -->
                </div>
            </div>
            <!-- /main sidebar -->
            <!-- Main content -->
            <div class="content-wrapper">
                <!-- Page header -->
                <div class="page-header page-header-default">
                    <div class="page-header-content">
                        <div class="page-title">
                            <h4><i class="icon-arrow-left52 position-left"></i> <span class="text-semibold">@ViewBag.dashboard</span> - @ViewBag.dashboardSmall</h4>
                        </div>
                        <div class="heading-elements">
                            <ul class="breadcrumb">
                                <li><a href="#"><i class="icon-home2 position-left"></i> @ViewBag.dashboard</a></li>
                                <li class="active">@ViewBag.dashboardSmall</li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!-- /page header -->
                <!-- Content area -->
                <div class="content">

                    @RenderBody()

                    <!-- Footer -->
                    <div class="footer text-muted">
                        &copy; 2020. <a href="#">Sales </a> by <a href="https://www.pamapersada.com/id/" target="_blank">PT. Tuah Turangga Agung</a>
                    </div>
                    <!-- /footer -->
                </div>
                <!-- /content area -->
            </div>
            <!-- /main content -->
        </div>
        <!-- /page content -->
    </div>
    <!-- /page container -->
</body>
</html>