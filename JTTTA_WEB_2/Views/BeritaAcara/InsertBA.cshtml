<style>
    .k-list-scroller {
        height: 170px !important;
        overflow-y: scroll !important;
    }

    .k-dropdown, .k-datepicker {
        width: 9.2em;
    }

    .k-dropdown-wrap, .k-picker-wrap {
        border-radius: 3px;
        height: 36px;
    }

        .k-picker-wrap .k-input {
            height: 32px;
            width: 100%;
        }

    .k-dropdown .k-input {
        padding-top: 7px;
    }

    .k-picker-wrap .k-icon {
        margin-top: 7px;
    }
</style>

<input type="hidden" id="urlPath" name="urlPath" value="@ViewBag.pathParent" />
